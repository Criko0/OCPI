  "$ref": "#/definitions/ocpi.2_2_1.commands.response",
  "definitions": {
    "ocpi.2_2_1.commands.response": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "result": {
              "type": "string",
              "enum": [
                "NOT_SUPPORTED",
                "REJECTED",
                "ACCEPTED",
                "UNKNOWN_SESSION"
              ]
            },
            "timeout": {
              "type": "integer",
              "minimum": 0
            },
            "message": {
              "anyOf": [
                {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "language": {
                        "type": "string",
                        "minLength": 2,
                        "maxLength": 2
                      },
                      "text": {
                        "type": "string",
                        "maxLength": 512
                      }
                    },
                    "required": [
                      "language",
                      "text"
                    ],
                    "additionalProperties": false
                  }
                },
                {
                  "type": "null"
                }
              ]
            }
          },
          "required": [
            "result",
            "timeout"
          ],
          "additionalProperties": false
        },
        "status_code": {
          "type": "integer",
          "minimum": 1000,
          "maximum": 1999
        },
        "status_message": {
          "type": [
            "string",
            "null"
          ]
        },
        "timestamp": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "data",
        "status_code",
        "timestamp"
      ],
      "additionalProperties": false
